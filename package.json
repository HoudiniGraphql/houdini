{
	"name": "houdini",
	"version": "0.13.10",
	"description": "The disappearing graphql client for SvelteKit",
	"workspaces": [
		"example",
		"."
	],
	"type": "module",
	"bin": "build/cmd.js",
	"exports": {
		"./preprocess": {
			"import": "./build/preprocess-esm/index.js",
			"require": "./build/preprocess-cjs/index.js"
		}
	},
	"scripts": {
		"tests": "NODE_ENV=test node --experimental-vm-modules node_modules/.bin/jest",
		"tests:watch": "node --experimental-vm-modules node_modules/.bin/jest  --watch",
		"build": "rm -rf build && tsc -p ./src/runtime && node ./build.mjs",
		"postinstall": "husky install",
		"changeset": "changeset",
		"version": "changeset version && yarn",
		"release": "yarn build && changeset publish"
	},
	"resolutions": {
		"typescript": "3.9.5"
	},
	"devDependencies": {
		"@babel/core": "^7.12.10",
		"@babel/preset-env": "^7.12.11",
		"@babel/preset-typescript": "^7.16.7",
		"@changesets/changelog-github": "^0.4.4",
		"@types/fs-extra": "^9.0.13",
		"@types/inquirer": "^7.3.1",
		"@types/jest": "^26.0.23",
		"@types/mkdirp": "^1.0.2",
		"babel-jest": "^26.6.3",
		"babel-plugin-search-and-replace": "1.1.0",
		"babel-plugin-transform-import-meta": "^2.1.1",
		"esbuild": "0.13.2",
		"esbuild-plugin-alias": "^0.2.1",
		"esbuild-plugin-replace": "^1.2.0",
		"fs-extra": "^10.0.1",
		"husky": "^6.0.0",
		"install": "^0.13.0",
		"jest": "^26.6.3",
		"lerna": "^3.22.1",
		"mock-fs": "^4.13.0",
		"npm": "^8.6.0",
		"prettier": "^2.0.5",
		"pretty-quick": "^3.1.1",
		"ts-ast-serializer": "^1.0.1",
		"ts-jest": "^26.0.0",
		"ts-node": "^9.1.1",
		"typescript": "^4.6.3"
	},
	"dependencies": {
		"@babel/parser": "^7.13.4",
		"@changesets/changelog-git": "^0.1.11",
		"@changesets/cli": "^2.22.0",
		"@graphql-tools/schema": "^8.3.7",
		"@types/mock-fs": "^4.13.0",
		"babylon": "^7.0.0-beta.47",
		"commander": "^7.1.0",
		"estree-walker": "^2.0.2",
		"glob": "^7.1.6",
		"global": "^4.4.0",
		"graphql": "^15.5.0",
		"inquirer": "^8.2.2",
		"mkdirp": "^1.0.4",
		"node-fetch": "^2.6.1",
		"recast": "^0.20.4",
		"remove": "^0.1.5",
		"rollup-plugin-typescript2": "^0.30.0",
		"svelte": "^3.34.0"
	}
}
