{
	"name": "example-kit",
	"private": true,
	"version": "0.13.0",
	"scripts": {
		"prepare": "svelte-kit sync",
		"dev": "npm run prepare && concurrently \"yarn web\" \"yarn api\" -n \"web,api\" -c \"green,magenta\"",
		"build": "svelte-kit build",
		"start": "svelte-kit start",
		"generate": "rm -rf \"\\$houdini\" && houdini generate",
		"web": "svelte-kit dev",
		"api": "node api.cjs"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "1.0.0-next.40",
		"@sveltejs/kit": "1.0.0-next.326",
		"concurrently": "7.1.0",
		"graphql": "15.5.0",
		"houdini": "^0.14.0",
		"svelte": "3.48.0",
		"svelte-preprocess": "4.10.6",
		"tslib": "2.4.0",
		"typescript": "4.6.3"
	},
	"type": "module",
	"dependencies": {
		"@kitql/vite-plugin-watch-and-run": "^0.3.0",
		"apollo-server": "^2.24.0",
		"graphql-relay": "^0.8.0",
		"subscriptions-transport-ws": "^0.9.18"
	}
}
