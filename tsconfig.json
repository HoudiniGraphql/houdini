{
	"compilerOptions": {
		"strict": true,
		"esModuleInterop": true,
		"lib": ["esnext", "dom"],
		"skipLibCheck": true,
		"downlevelIteration": true,
		"target": "es5",
		"sourceMap": false,
		"declaration": true,
		"paths": {
			"~/common": ["./src/common"],
			"~/runtime": ["./src/runtime"],
			"~/runtime/*": ["./src/runtime/*"]
		},
		"typeRoots": ["./node_modules/@types", "."]
	}
}
