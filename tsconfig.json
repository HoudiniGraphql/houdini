{
	"include": ["./src/runtime/**/*"],
	"output": "./build/runtime-types",
	"compilerOptions": {
		"outDir": "build",
		"strict": true,
		"esModuleInterop": true,
		"lib": ["esnext", "dom"],
		"skipLibCheck": true,
		"downlevelIteration": true,
		"target": "es5",
		"sourceMap": true,
		"declaration": true,
		"emitDeclarationOnly": true
	}
}
